.choose-address .form-item{
  height: 116rpx;
  padding-left: 31.25rpx;
  border-bottom: 1px solid #d9d9d9;
  display: flex;
  align-items: center;
  padding-right: 31.25rpx;
}
.choose-address .input{
  flex: 1;
  height: 44rpx;
  line-height: 44rpx;
  overflow: hidden;
}
.choose-address .add-form{
  background: #fff;
  width: 100%;
  height: auto;
  overflow: hidden;
}
ec-canvas {
  width: 100%;
  height: 100%;
}
.container {
  position: static;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0; 
  z-index: -1;

  display: flex;
  background-color: #fff;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.map_name {
  height: 85rpx;
  width: 100%;
  font-size: 18px;
  font-weight: bold;
  color: #a3a3a3;
  display: flex;
  align-items: center;
  padding-left: 31.25rpx;
  background-color: #f9f9f9;
}
.hot_talk {
  position: relative;
  color: #a0a0a0;
  font-size: 18px;
  font-weight: bold;
  padding-left: 31.25rpx;
  display: flex;
  align-items: center;
  border-top: 1px solid #eeeeee;
  height: 100rpx;
  width: 100%;
}
.hot_talk_title {
  position: relative;
  color: #a0a0a0;
  font-size: 14px;
  font-weight: bold;
  padding-left: 31.25rpx;
  display: flex;
  align-items: center;
  border-top: 1px solid #eeeeee;
  height: 100rpx;
  width: 100%;
}

#mychart-dom-map {
  position: relative;
  width: 100%;
  padding-top: 20rpx;
  height: 800rpx;
  }

.picker{
  font-size: 16px;
  font-weight: bold;
  color: #909090;
}